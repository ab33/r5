{
  "name": "Reveal5",
  "version": "0.1",
  "description": "Reveals media sources of HTML5 media elements on the page.",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "64": "icon64.png",
            "128": "icon128.png" },
  "content_scripts": [ {
      "js": [ "cs.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "all_frames": true
  } ],
  "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Reveal HTML5 media sources",
      "popup": "popup.html"
   },
  "permissions": [
      "tabs"
  ]
}
